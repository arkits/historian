{
    "manifest_version": 2,
    "name": "Historian - Chrome History Agent",
    "version": "0.1",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "img/favicon-16x16.png",
        "32": "img/favicon-32x32.png",
        "128": "img/icon.png"
    },
    "options_page": "options/options.html"
}